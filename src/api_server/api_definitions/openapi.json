    "components": {
        "schemas": {
            "AppointmentQueryRequest": {
                "properties": {
                    "hospital_name": {
                        "type": "string",
                        "title": "Hospital Name",
                        "description": "医院名称, 要求医院存在"
                    },
                    "department_name": {
                        "type": "string",
                        "title": "Department Name",
                        "description": "科室名称, 要求科室存在"
                    },
                    "appointment_time": {
                        "type": "string",
                        "title": "Appointment Time",
                        "description": "预约时间"
                    }
                },
                "type": "object",
                "required": [
                    "hospital_name",
                    "department_name",
                    "appointment_time"
                ],
                "title": "AppointmentQueryRequest"
            },
            "CityRequest": {
                "properties": {
                    "city": {
                        "type": "string",
                        "title": "City"
                    }
                },
                "type": "object",
                "required": [
                    "city"
                ],
                "title": "CityRequest"
            },
            "CountryRequest": {
                "properties": {
                    "country": {
                        "type": "string",
                        "title": "Country",
                        "description": "目标国家"
                    }
                },
                "type": "object",
                "required": [
                    "country"
                ],
                "title": "CountryRequest"
            },
            "DepartmentCheckRequest": {
                "properties": {
                    "department_name": {
                        "type": "string",
                        "title": "Department Name"
                    },
                    "hospital_name": {
                        "type": "string",
                        "title": "Hospital Name"
                    }
                },
                "type": "object",
                "required": [
                    "department_name",
                    "hospital_name"
                ],
                "title": "DepartmentCheckRequest"
            },
            "HTTPValidationError": {
                "properties": {
                    "detail": {
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "title": "HTTPValidationError"
            },
            "HospitalCheckRequest": {
                "properties": {
                    "hospital_name": {
                        "type": "string",
                        "title": "Hospital Name"
                    }
                },
                "type": "object",
                "required": [
                    "hospital_name"
                ],
                "title": "HospitalCheckRequest"
            },
            "HospitalRegistrationRequest": {
                "properties": {
                    "id_number": {
                        "type": "string",
                        "title": "Id Number"
                    },
                    "registration_type": {
                        "type": "string",
                        "title": "Registration Type",
                        "description": "专家号 或 普通号"
                    },
                    "hospital_name": {
                        "type": "string",
                        "title": "Hospital Name"
                    },
                    "department_name": {
                        "type": "string",
                        "title": "Department Name"
                    },
                    "appointment_time": {
                        "type": "string",
                        "title": "Appointment Time"
                    }
                },
                "type": "object",
                "required": [
                    "id_number",
                    "registration_type",
                    "hospital_name",
                    "department_name",
                    "appointment_time"
                ],
                "title": "HospitalRegistrationRequest"
            },
            "IdentityQueryRequest": {
                "properties": {
                    "CardID": {
                        "type": "string",
                        "title": "Cardid"
                    }
                },
                "type": "object",
                "required": [
                    "CardID"
                ],
                "title": "IdentityQueryRequest"
            },
            "NewsQueryRequest": {
                "properties": {
                    "news_location": {
                        "type": "string",
                        "enum": [
                            "北京",
                            "上海",
                            "广州",
                            "深圳",
                            "杭州",
                            "成都",
                            "武汉",
                            "重庆"
                        ],
                        "title": "News Location"
                    },
                    "news_type": {
                        "type": "string",
                        "enum": [
                            "政治",
                            "经济",
                            "体育",
                            "娱乐",
                            "科技",
                            "教育",
                            "健康",
                            "国际"
                        ],
                        "title": "News Type"
                    },
                    "news_time": {
                        "type": "string",
                        "title": "News Time"
                    }
                },
                "type": "object",
                "required": [
                    "news_location",
                    "news_type",
                    "news_time"
                ],
                "title": "NewsQueryRequest"
            },
            "OtherHospitalRecommendationRequest": {
                "properties": {
                    "department_name": {
                        "type": "string",
                        "title": "Department Name"
                    },
                    "appointment_time": {
                        "type": "string",
                        "title": "Appointment Time"
                    }
                },
                "type": "object",
                "required": [
                    "department_name",
                    "appointment_time"
                ],
                "title": "OtherHospitalRecommendationRequest"
            },
            "OtherHospitalRegistrationRequest": {
                "properties": {
                    "id_number": {
                        "type": "string",
                        "title": "Id Number"
                    },
                    "hospital_name": {
                        "type": "string",
                        "title": "Hospital Name"
                    },
                    "doctor_name": {
                        "type": "string",
                        "title": "Doctor Name"
                    }
                },
                "type": "object",
                "required": [
                    "id_number",
                    "hospital_name",
                    "doctor_name"
                ],
                "title": "OtherHospitalRegistrationRequest"
            },
            "QueryInvoiceMethodRequest": {
                "properties": {
                    "order_id": {
                        "type": "integer",
                        "title": "Order Id"
                    }
                },
                "type": "object",
                "required": [
                    "order_id"
                ],
                "title": "QueryInvoiceMethodRequest"
            },
            "QueryInvoiceProgressRequest": {
                "properties": {
                    "name": {
                        "type": "string",
                        "title": "Name"
                    }
                },
                "type": "object",
                "required": [
                    "name"
                ],
                "title": "QueryInvoiceProgressRequest"
            },
            "QueryMemberStatusRequest": {
                "properties": {
                    "card_id": {
                        "type": "integer",
                        "title": "Card Id"
                    }
                },
                "type": "object",
                "required": [
                    "card_id"
                ],
                "title": "QueryMemberStatusRequest"
            },
            "ShippingRequest": {
                "properties": {
                    "country": {
                        "type": "string",
                        "title": "Country",
                        "description": "目标国家"
                    },
                    "goods": {
                        "type": "string",
                        "title": "Goods",
                        "description": "用户要邮寄的物品"
                    }
                },
                "type": "object",
                "required": [
                    "country",
                    "goods"
                ],
                "title": "ShippingRequest"
            },
            "ValidationError": {
                "properties": {
                    "loc": {
                        "items": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "integer"
                                }
                            ]
                        },
                        "type": "array",
                        "title": "Location"
                    },
                    "msg": {
                        "type": "string",
                        "title": "Message"
                    },
                    "type": {
                        "type": "string",
                        "title": "Error Type"
                    }
                },
                "type": "object",
                "required": [
                    "loc",
                    "msg",
                    "type"
                ],
                "title": "ValidationError"
            }
        }
    }
}
