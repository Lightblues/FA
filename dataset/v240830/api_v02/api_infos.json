  {
    "name": "query_appointment",
    "description": "指定时间号源查询",
    "parameters": {
      "type": "object",
      "properties": {
        "hospital_name": {
          "type": "string",
          "name": "医院名称",
          "description": "要求医院存在"
        },
        "department_name": {
          "type": "string",
          "name": "科室名称",
          "description": "要求科室存在"
        },
        "appointment_time": {
          "type": "string",
          "name": "预约时间",
          "description": "预约时间"
        }
      },
      "required": [
        "hospital_name",
        "department_name",
        "appointment_time"
      ]
    },
    "response": {
      "type": "object",
      "properties": {
        "available_slots": {
          "type": "integer",
          "name": "可用号源数量",
          "description": "可用号源数量"
        },
        "available_list": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "hospital_name": {
                "type": "string"
              },
              "department_name": {
                "type": "string"
              },
              "doctor_name": {
                "type": "string"
              },
              "time_period": {
                "type": "string",
                "description": "时间段, 例如 '09:00-10:00'"
              },
              "is_specialist": {
                "type": "boolean"
              }
            }
          },
          "name": "可选列表",
          "description": "可选列表"
        },
        "specialist_count": {
          "type": "integer",
          "name": "专家号数量",
          "description": "专家号数量"
        },
        "general_count": {
          "type": "integer",
          "name": "普通号数量",
          "description": "普通号数量"
        }
      }
    },
    "URL": "http://www.easonsi.site:9390/query_appointment",
    "Method": "POST",
    "from": "000-114挂号"
  },
